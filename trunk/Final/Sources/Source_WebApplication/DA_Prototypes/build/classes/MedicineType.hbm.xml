<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.med.dic.model.MedicineType" table="medicine_type">
	    <!-- id -->
		<id name="medTypeId" type="int">
			<column name="ID" />
			<generator class="identity" />
		</id>
		
		<!-- loai thuoc -->
		<property name="medTypeName" type="string">
			<column name="NAME" length="50" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>