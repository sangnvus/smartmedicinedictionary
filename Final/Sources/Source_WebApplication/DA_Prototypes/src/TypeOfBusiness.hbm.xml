<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.med.dic.model.TypeOfBusiness" table="type_of_business">
	    <id name="typeBusinessId" type="int">
			<column name="ID" />
			<generator class="identity" />
		</id>
		<property name="typeBusinessName" type="string">
		    <column name="TYPE_OF_BUSINESS" length="100" not-null="true" />
		</property>
	</class>
</hibernate-mapping>